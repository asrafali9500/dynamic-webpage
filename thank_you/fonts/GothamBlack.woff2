	<script>
        $("input.mask").each((i,ele)=>{
         let clone=$(ele).clone(false)
         clone.attr("type","text")
         clone.attr("name","tmp")
         clone.attr("id","tmp")
         let ele1=$(ele)
                 
         clone.val("Monthly Electricity Bill (Rs)*")
         //clone.val(Number(ele1.val()).toLocaleString("en"))
         $(ele).after(clone)
         $(ele).hide()
         
         clone.mouseenter(()=>{
     
           ele1.show()
           clone.hide()
         })
         setInterval(()=>{
           let newv=Number(ele1.val()).toLocaleString("en-IN")
           
           if (newv != 0) {
                if(clone.val()!=newv){
                     clone.val(newv)
                   }
            }
            else 
            clone.val("Monthly Electricity Bill (Rs)*")
         },10)
     
         $(ele).mouseleave(()=>{
           $(clone).show()
           $(ele1).hide()
         })
         
     
       })
     </script>
